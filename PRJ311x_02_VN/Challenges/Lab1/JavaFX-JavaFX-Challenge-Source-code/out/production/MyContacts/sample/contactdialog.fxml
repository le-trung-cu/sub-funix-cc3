<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="sample.ContactController">
    <headerText>Fill in the information for the new Contact</headerText>
    <content>
        <GridPane vgap="10" hgap="10">
            <Label text="Fist Name: " GridPane.rowIndex="0" GridPane.columnIndex="0"/>
            <TextField fx:id="firstNameFiled" GridPane.rowIndex="0" GridPane.columnIndex="1"/>

            <Label text="Last Name: " GridPane.rowIndex="1" GridPane.columnIndex="0"/>
            <TextField fx:id="lastNameFiled" GridPane.rowIndex="1" GridPane.columnIndex="1"/>

            <Label text="Phone Number: " GridPane.rowIndex="2" GridPane.columnIndex="0"/>
            <TextField fx:id="phoneNumberFiled" GridPane.rowIndex="2" GridPane.columnIndex="1"/>

            <Label text="Notes: " GridPane.rowIndex="3" GridPane.columnIndex="0"/>
            <TextField fx:id="notesFiled" GridPane.rowIndex="3" GridPane.columnIndex="1"/>

        </GridPane>
    </content>
</DialogPane>
